{
  "manifest_version": 3,
  "name": "Simple Redirector",
  "version": "1.0",
  "description": "Redirects from a specific page to another.",
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "*://*.google.com/*",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
