{
    "manifest_version": 3,
    "name": "URL Redirector",
    "version": "1.0",
    "description": "Redirects a specific URL to a new URL",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "*://friendsbalt.myschoolapp.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  