{
  "manifest_version": 3,
  "name": "Simple Redirector",
  "version": "1.0",
  "description": "Redirects from a specific page to another.",
  "permissions": [
    "webNavigation",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": ""
  },
  "icons": {}
}
