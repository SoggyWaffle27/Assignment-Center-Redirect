{
  "manifest_version": 3,
  "name": "Simple Redirector",
  "version": "1.0",
  "description": "Redirects from a specific page to another.",
  "permissions": [
    "webNavigation",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://friendsbalt.myschoolapp.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
